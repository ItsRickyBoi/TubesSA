// PDA for b^n a^m
digraph {
	q0 [label="q0: Start"]
	q1 [label="q1: Process b's"]
	q2 [label="q2: Process a's"]
	q3 [label="q3: Accept" shape=doublecircle]
	q0 -> q1 [label="b, ε -> X"]
	q1 -> q1 [label="b, ε -> ε"]
	q1 -> q2 [label="a, ε -> ε"]
	q2 -> q2 [label="a, ε -> ε"]
	q2 -> q3 [label="ε, ε -> ε" constraint=false]
}
